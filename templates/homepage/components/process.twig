          img: 'https://cdn-ilcpbgd.nitrocdn.com/cJgSbGIAEdkxfzcRRCssjDxHYSpdiDcw/assets/images/optimized/rev-7dc58c8/clickmedialab.com/wp-content/uploads/2025/05/Google-people-Card-.webp',
